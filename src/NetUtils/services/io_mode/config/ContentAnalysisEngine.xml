                        <savapiNotifyOfficeMacroAutostart>0</savapiNotifyOfficeMacroAutostart>
                        <heurMacro>1</heurMacro>
                        <heurLevel>2</heurLevel>
                        <timeout>300</timeout>
                        <repair>0</repair>
                    </engine>
                </engines>
